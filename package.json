{
  "name": "dhf-pay-js",
  "version": "1.0.10",
  "description": "DHF Pay js sdk",
  "main": "lib/DHFPay.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json  src/e2e.test.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "Blokchain",
    "Casper"
  ],
  "author": "Konstantin Lazukin",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "dotenv": "^16.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.0"
  }
}
